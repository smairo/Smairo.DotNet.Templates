{
  "UseSwagger": true,
  "Authentication": {
    "UseAuthentication": false,
    "Authority": "...",
    "Audience": "...",
    "RequireHttpsMetadata": false
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Smairo.Template.Api": "Debug"
      }
    },
    "WriteTo": [ { "Name": "Console" } ],
