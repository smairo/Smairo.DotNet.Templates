{
  "UseSwagger": false,
  "Authentication": {
    "UseAuthentication": true,
    "Authority": "...",
    "RequireHttpsMetadata": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "IdentityServer4.AccessTokenValidation": "Warning",
        "Microsoft.AspNetCore.Authorization": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Logentries",
        "Args": { "token": "..." }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "ConnectionStrings": {
    // You should use key vault
    "DatabaseContext": "Server=(localdb)\\mssqllocaldb;Database=Smairo.Template;Trusted_Connection=True;"
  } 
}